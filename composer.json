  "type": "magento2-module",
  "version": "1.0.0",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "require": {
    "php": ">=5.3.3"
  },
  "authors": [
    {
      "name": "PagaDevcomm",
      "email": "devcomm@paga.com",
      "homepage": "https://developer.paga.com/",
      "role": "Fintech"
    }
  ],
  "config": {
    "platform": {
        "php": "7.2.29"
    },
    "optimize-autoloader": true,
